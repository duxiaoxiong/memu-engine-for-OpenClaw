{
  "id": "memu-engine",
  "name": "memU Agentic Engine",
  "description": "memU agentic memory layer (SQLModel + Vector)",
  "kind": "memory",
  "configSchema": {
    "type": "object",
    "properties": {
      "embedding": { "type": "object" },
      "extraction": { "type": "object" },
      "ingest": {
        "type": "object",
        "properties": {
          "includeDefaultPaths": {
            "type": "boolean",
            "default": true,
            "description": "If true, ingest OpenClaw workspace root *.md and workspace/memory/*.md by default."
          },
          "extraPaths": {
            "type": "array",
            "items": { "type": "string" },
            "default": [],
            "description": "Extra directories/files to ingest Markdown from."
          }
        },
        "additionalProperties": true
      }
    }
  }
}
