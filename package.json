{
  "name": "memu-engine",
  "version": "0.2.3",
  "description": "Atomic memory engine for OpenClaw powered by MemU. Features immutable parts for zero redundant token cost.",
  "main": "index.ts",
  "files": [
    "index.ts",
    "openclaw.plugin.json",
    "python/pyproject.toml",
    "python/uv.lock",
    "python/watch_sync.py",
    "python/auto_sync.py",
    "python/convert_sessions.py",
    "python/docs_ingest.py",
    "python/scripts/search.py",
    "python/scripts/get.py",
    "python/scripts/flush.py",
    "python/src/memu/**/*.py",
    "python/src/memu/**/*.pyi",
    "python/src/memu/**/*.so",
    "README.md",
    "README_ZH.md",
    "MEMU_PARAMETERS.md",
    "LICENSE",
    "NOTICE"
  ],
  "openclaw": {
