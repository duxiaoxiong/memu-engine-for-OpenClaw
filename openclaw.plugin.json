{
  "id": "memu-engine",
  "name": "memU Agentic Engine",
  "description": "memU agentic memory layer (SQLModel + Vector)",
  "kind": "memory",
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "embedding": {
        "type": "object",
        "properties": {
          "provider": { 
            "type": "string", 
            "default": "openai", 
            "enum": ["openai", "doubao", "siliconflow"],
            "description": "Embedding provider" 
          },
          "apiKey": { "type": "string", "description": "API Key" },
          "baseUrl": { "type": "string", "description": "Base URL" },
          "model": { "type": "string", "description": "Model ID" }
        },
        "required": ["apiKey"]
      },
      "extraction": {
        "type": "object",
        "description": "LLM used for memory extraction and summarization",
        "properties": {
          "provider": { 
            "type": "string", 
            "default": "openai",
            "enum": ["openai", "doubao", "grok", "openrouter"],
            "description": "LLM provider"
          },
          "apiKey": { "type": "string", "description": "API Key (optional, defaults to embedding key if empty)" },
          "baseUrl": { "type": "string", "description": "Base URL" },
          "model": { "type": "string", "description": "Chat Model ID" }
        }
      },
      "ingest": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "timeoutSeconds": { "type": "number", "default": 600 },
          "extraPaths": { 
            "type": "array", 
            "items": { "type": "string" },
            "description": "List of absolute paths to documentation directories or files to ingest."
          }
        }
      }
    },
    "required": ["embedding"]
  },
  "uiHints": {
    "embedding.provider": {
      "label": "Provider Type",
      "help": "Currently supports 'openai' compatible providers (SiliconFlow, OpenAI, LocalAI, etc.)"
    },
    "embedding.apiKey": {
      "label": "API Key",
      "sensitive": true,
      "placeholder": "sk-..."
    },
    "embedding.baseUrl": {
      "label": "Base URL",
      "placeholder": "https://api.openai.com/v1"
    },
    "embedding.model": {
      "label": "Model ID",
      "placeholder": "text-embedding-3-small"
    },
    "ingest.extraPaths": {
      "label": "Extra Documentation Paths",
      "help": "Directories containing Markdown/Text files to auto-ingest into memory.",
      "placeholder": "/home/user/my-docs"
    }
  }
}
